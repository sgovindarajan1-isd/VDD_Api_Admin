<div class="container row-wrapper">
    <div class="centerColumn">
        <div class="content clearfix">
            <input id="skipUnSavedChanges" type="hidden" value="0">

            <div class="leftContent">
                <div class="leftBox leftBoxMenu">
                    <h5>System Lists</h5>
                    <div class="menu">

                        <a id="sidemenu_PendingAssignment" class="leftNavItem  leftNavItemActive">
                            <span id="span_countPendingAssignment"class="badge pull-right" style="margin-right:3px;"></span>
                            Application Pending Assignment
                        </a>
                        <a id="sidemenu_PendingMyApproval"  class="leftNavItem ">
                            <span id="span_countPendingMyApproval" class="badge pull-right" style="margin-right:3px;"></span>
                            Pending My Approval
                        </a>

                    </div>
                </div>

                <div class="leftBox leftBoxMenu">
                    <h5>User Lists</h5>
                    <div class="menu">
                        <a href="./ApplicationList_OpenApplications.html" class="leftNavItem ">
                            <span id="span_appPendingOver60Days" class="badge pull-right" style="margin-right:3px;"></span>
                            Applications Over 60 Days
                        </a>

                    </div>
                </div>

                <div class="topMargin">
                    <a href="./User Account_ApplicationLists.html">
                        <span class="fa smallRightMargin fa-gear"></span><b>Manage User Lists</b>
                    </a>
                </div>

            </div>


            <!--------------------------------->

            <div class="mainContent">
                <div class="mainBox">
                    <div id="customizeResults">
                        <a class="pull-right nonFormSubmit" href="#" data-url="#">
                            <strong>Customize Results</strong>
                        </a>
                        <h2 id="heading_applicationlist" class="smallBottomMargin">Application Pending Assignment</h2>
                    </div>

                    <div>
                        <strong>Application Age: </strong>

                        <div class="well well-xxs smallRightMargin" style="display:inline-block; ">
                            <span class="fa fa-fw fa-circle text-green" aria-hidden="true"> </span>
                            <span class="">0 - 15 days </span>
                        </div>
                        <div class="well well-xxs smallRightMargin" style="display:inline-block; ">
                            <span class="fa fa-fw fa-circle text-blue" aria-hidden="true"> </span>
                            <span class="">16-30 days </span>
                        </div>
                        <div class="well well-xxs smallRightMargin" style="display:inline-block; ">
                            <span class="fa fa-fw fa-circle text-orange" aria-hidden="true"> </span>
                            <span class="">31-60 days </span>
                        </div>
                        <div class="well well-xxs smallRightMargin" style="display:inline-block; ">
                            <span class="fa fa-fw fa-circle text-red" aria-hidden="true"> </span>
                            <span class="">61+ days </span>
                        </div>
                    </div>


                    <table id="ddGrid" class="w-auto mdl-data-table cell-border" cellspacing="0" style="border-collapse:collapse; border: 1px solid black; width:100%">
                        <thead>
                            <tr class="button-header-color">
                                @*<th></th>*@
                                <th>ConfirmationNum</th>
                                <th>VendorName</th>
                                <th>ReceivedDate</th>
                                <th>AssignedDate</th>
                                <th>Application Age</th>
                                <th>Status</th>
                            </tr>
                        </thead>
                    </table>


                </div>
            </div>




            <!--------------------------------->
        </div>
    </div>
</div>