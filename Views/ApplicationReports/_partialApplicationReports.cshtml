    <!-- Claims Summary Field-->
    <div class="card mb-3 mt-3" id="ApplicationCountSection" style="display:none">
        <div class="card-header">
            <h6 class="m-0 font-weight-bold text-primary">Applications</h6>
        </div>
        <div class="card-body">
            <div class="row justify-content-md-center">
                <div class="col-2 mb-4">
                    <div class="card border-left-primary shadow h-100 py-2">
                        <div class="card-body">
                            <div class="row no-gutters align-items-center">
                                <div class="col mr-2">
                                    <div class="font-weight-bold text-primary text-uppercase mb-1">New</div>
                                    <div id="card_New" class="h5 mb-0 font-weight-bold text-gray-800"></div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-2 mb-4">
                    <div class="card border-left-success shadow h-100 py-2">
                        <div class="card-body">
                            <div class="row no-gutters align-items-center">
                                <div class="col mr-2">
                                    <div class="font-weight-bold text-success text-uppercase mb-1">Processor Review</div>
                                    <div id="card_ProcessorReview" class="h5 mb-0 font-weight-bold text-gray-800"></div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-2 mb-4">
                    <div class="card border-left-info shadow h-100 py-2">
                        <div class="card-body">
                            <div class="row no-gutters align-items-center">
                                <div class="col mr-2">
                                    <div class="font-weight-bold text-info text-uppercase mb-1">Pending Vendor Confirmation</div>
                                    <div id="card_PendingVendorConfirmation" class="h5 mb-0 mr-3 font-weight-bold text-gray-800"></div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-2 mb-4">
                    <div class="card border-left-warning shadow h-100 py-2">
                        <div class="card-body">
                            <div class="row no-gutters align-items-center">
                                <div class="col mr-2">
                                    <div class="font-weight-bold text-warning text-uppercase mb-1">Supervisor Review</div>
                                    <div id="card_SupervisorReview" class="h5 mb-0 font-weight-bold text-gray-800"></div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row justify-content-md-center">
                <div class="col-2 mb-4">
                    <div class="card border-left-orange shadow h-100 py-2">
                        <div class="card-body">
                            <div class="row no-gutters align-items-center">
                                <div class="col mr-2">
                                    <div class="font-weight-bold text-orange text-uppercase mb-1">Approved</div>
                                    <div id="card_Approved" class="h5 mb-0 font-weight-bold text-gray-800 align-text-bottom"></div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-2 mb-4">
                    <div class="card border-left-danger shadow h-100 py-2">
                        <div class="card-body">
                            <div class="row no-gutters align-items-center">
                                <div class="col mr-2">
                                    <div class="font-weight-bold text-danger text-uppercase mb-1">Rejected</div>
                                    <div id="card_Rejected" class="h5 mb-0 font-weight-bold text-gray-800">0</div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>


    <div id="divClaimsResults">
        <table id="ddGrid" class="w-auto mdl-data-table cell-border" cellspacing="0" style="font-size: 12px; border-collapse:collapse; border: 1px solid black; width:100%">
            <thead>
                <tr class="button-header-color">
                    <th>ConfirmationNum</th>
                    <th>VendorName</th>
                    <th>ReceivedDate</th>
                    <th>AssignedDate</th>
                    <th>Application Age</th>
                    <th>StatusDesc</th>
                </tr>
            </thead>
        </table>
    </div>


    <div id="divVCMResults">
        <table id="ddVCMGrid" class="w-auto mdl-data-table cell-border" cellspacing="0" style="font-size: 12px; border-collapse:collapse; border: 1px solid black; width:100%">
            <thead>
                <tr class="button-header-color">
                    <th>ConfirmationNum</th>
                    <th>VendorNumber</th>
                    <th>VendorName</th>
                    <th>ReceivedDate</th>
                    <th>AttachmentCount</th>
                    <th>DocumentCreateDate</th>
                </tr>
            </thead>
        </table>
    </div>

