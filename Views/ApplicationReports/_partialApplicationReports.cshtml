            <div id="panel_Application">
                <h2>Application Type, Status, User ID and Date Report</h2>
                <div class="card mb-3 mt-3">
                    <div class="card-header">
                    </div>
                    <div class="card-body">
                        <div class="container">
                            <div class="col-lg-3 col-md-12 col-sm-12">
                                <div class="form-group">
                                    <h6>Types of Applications</h6>
                                    <div class="input-group w-100">
                                        <select id="selectApplicationType" class="select-form-control">
                                            <option value=""></option>
                                            @*<option value="2" selected=""></option>
                                                <option value="2">ACCHasdfasd</option>*@

                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-12 col-sm-12">
                                <div class="form-group">
                                    <h6>Status</h6>
                                    <div class="input-group w-100">
                                        <select id="selectStatus" class="select-form-control">
                                            <option value=""></option>
                                            @*<option value="2" selected=""></option>
                                                <option value="2">Newadsfasdf</option>*@

                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-12 col-sm-12">
                                <div class="form-group">
                                    <h6>User ID</h6>
                                    <div class="input-group w-100">
                                        <select id="selectUser" class="select-form-control">
                                            <option value=""></option>
                                            @* <option value="2" selected=""></option>
                                                <option value="2">Ellaine Mallariqwerwqerqw</option>*@

                                        </select>
                                    </div>
                                </div>
                            </div>
