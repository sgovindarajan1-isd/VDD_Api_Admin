    <div id="wp_content">
        <div id="liNavigation" class="row-wrapper row  row-home-gray">

        </div>

        <div id="grey-info-bar" class="content-font row-wrapper">
            <div class="logo-bar-wrapper1">
                <div class="col-xs-12">
                    <h3 id="lbl_header">
                        Direct Deposit Management System
                        <span id="btn_loginLock" class="fa fa-expeditedssl fa-right" style="font-size: 22px;"></span>

                        <span id="id_userName" class="fa fa-expeditedssl fa-right" style="float: right; font-size: 22px;"></span>
                    </h3>

                    <div class="header-line header-line-purple"><span></span></div>
                </div>
            </div>
        </div>

        <div id="alertModal" class="modal fade" tabindex="-1">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-body">
                        <p id="alertContent"></p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" id="btn_alertOK" class="btn button-header-color">Ok</button>
                    </div>
                </div>
            </div>
        </div>

        <div class="modal" id="logoutModal" tabindex="-1" role="dialog">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title">LOGOUT</h5>
                    </div>
                    <div class="modal-body" style="text-align:center">
                        Are you sure you want to logout?
                    </div>
                    <div class="modal-footer">
                        <button type="button" style="text-align:center" class="btn btn-default  btn-lg" data-dismiss="modal">No</button>
                        <button id="btn_logout" type="button" class="btn btn-primary btn-lg">Yes</button>
                    </div>
                </div>
            </div>
        </div>

        <div id="layoutBody" class="row-wrapper layoutBody">
            @RenderBody()
        </div>

        <!--new footer -->

        <div id="wp_footer-wrapper" class="row-wrapper">

            <div id="wp_footer-widget-bar" class="widget" style="display:flow-root">
                <div class="wp_container">
                    <section class="widget-area">
                        <div class="widget">
                            <div class="textwidget"><img style="PADDING-LEFT: 10px; PADDING-TOP: 0px" src="/Content/Images/LACountySeal.png" alt="County Seal"><br><p style="PADDING-LEFT: 10px; padding-top:10px;"><b>Enriching Lives . . .</b></p></div>
                        </div>
                    </section>
                    <section class="widget-area">
                        <div class="widget">
                            @*<h4 class="heading-font">DO YOU NEED HELP?</h4>*@
                            <div class="menu-do-you-need-help-container">
                                <ul id="menu-do-you-need-help" class="menu">
                                    <li id="menu-item-2431" class="menu-item menu-item-type-custom menu-item-object-custom menu-item-2431"><a target="_blank" href="">Profile<span class="s"></span></a></li>
                                </ul>
                            </div>
                        </div>
                    </section>
                    <section class="widget-area">
                        <div class="widget">
                            @* <h4 class="heading-font">ABOUT US</h4>*@
                            <div class="menu-about-us-container">
                                <ul id="menu-about-us" class="menu">
                                    <li id="menu-item-3043" class="menu-item menu-item-type-custom menu-item-object-custom menu-item-3043"><a href="">Logout</a></li>
                                </ul>
                            </div>
                        </div>
                    </section>
                    <section class="widget-area">
                        <div class="widget">
                            @*<h4 class="heading-font">SITE INFO</h4>*@
                            <div class="menu-site-info-container">
                                <ul id="menu-site-info" class="menu">
                                    <li id="menu-item-2659" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-2659"><a href="">Help</a></li>
                                </ul>
                            </div>
                        </div>
                    </section>

                </div><!--.container-->
            </div><!--#footer-widget-bar-->
        </div> @*"wp_footer-wrapper"*@
        <!--new footer  -->


    </div>  @*wp_content*@

    @Scripts.Render("~/jsbundles/jquery")

    <script>
        var hostdomainUrl = '@System.Configuration.ConfigurationManager.AppSettings["HostDomainUrl"]';
    </script>

    @RenderSection("scripts", required: false)

</body>
</html>


