                <div style="float:right">
                    <div>
                        <div data-toggle="modal" class="dropdown" style="z-index: 2000; cursor: pointer">
                            <span id="img_username" class="fa fa-user-circle" style="color:rgba(41,213,236,1)">
                                <label id="lbl_userName" aria-label="Close" data-toggle="modal" data-target="#logoutModal" style="color:rgba(41,213,236,1); font-family: Arial, Helvetica, sans-serif;  cursor: pointer; font-size:medium;font-weight:initial" />
                            </span>
                        </div>
                    </div>
                </div>
            </div>


